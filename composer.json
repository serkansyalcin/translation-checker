{
    "name": "serkansyalcin/translation-checker",
    "description": "A Laravel package to check for missing translations across languages.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TranslationChecker\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                 "TranslationChecker\\Providers\\LangCheckerServiceProvider"
            ]
        }
    },
    "keywords": ["laravel", "localization", "translation", "missing keys", "lang"]
}